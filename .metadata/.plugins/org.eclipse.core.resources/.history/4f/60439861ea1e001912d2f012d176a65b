
public class Ville {
	public String nom;
	public int nombreHabitants;
	public String pays;
	public int sup;
	
	
	public Ville() {
		super();
		// TODO Auto-generated constructor stub
	}


	public Ville(String nom, int nombreHabitants, String pays, int sup) {
		super();
		this.nom = nom;
		this.nombreHabitants = nombreHabitants;
		this.pays = pays;
		this.sup = sup;
	}
	
	
	
	

}


